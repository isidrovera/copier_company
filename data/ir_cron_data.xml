<odoo>
    <data noupdate="1">
        <record id="ir_cron_backup" model="ir.cron">
            <field name="name">Automated Backup</field>
            <field name="model_id" ref="model_backup_config_settings"/>
            <field name="state">code</field>
            <field name="code">model.create_backup()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
