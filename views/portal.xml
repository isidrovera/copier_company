<odoo>
    <data>
        <template id="descargas_portal_my_home" inherit_id="portal.portal_my_home" customize_show="True">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="title">Descargas</t>
                    <t t-set="url" t-value="'/my/descargas'"/>
                    <t t-set="descargas_count" t-value="descargas_count"/>
                </t>
            </xpath>
        </template>
    </data>
</odoo>

